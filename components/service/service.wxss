/* components/service/service.wxss */
.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}

.room-id {
  width: 100%;
  height: 10vh;
  line-height: 10vh;
  padding-left: 5vw;
  padding-right: 5vw;
}

.container {
  background-color: #f5f5f5;
  height: 92vh;
  width: 100%;
}

.services-background {
  padding-top: 2vh;
  background-color: #f5f5f5;
  width: 92vw;
  height: auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding-left: 4vw;
  padding-right: 4vw;
}

.service-panel {
  width: 45vw;
  height: 14vh;
  margin-top:1vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 15rpx;
  background-color: white;
  justify-content: space-between;
  padding-top: 5vh;
  padding-bottom: 5vh;
}

.service-icon {
  width: 9vh;
  height: 9vh;
}

.service-text {
  font-family: MicrosoftYaHei;
	font-size: 2vh;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0vh;
	color: #585858;
  text-align: center;
}

.service-row {
  width: 100%;
  float: left;
}